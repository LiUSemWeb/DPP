  <rdf:Description rdf:about="http://w3id.org/dppo/ontology/dpp-odp/">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <owl:versionIRI rdf:resource="http://w3id.org/dppo/ontology/dpp-odp/0.1/"/>
    <dcterms:contributor>Eva Blomqvist</dcterms:contributor>
    <dcterms:contributor>Huanyu Li</dcterms:contributor>
    <dcterms:contributor>Robin Keskisärkkä</dcterms:contributor>
    <dcterms:created>2023-10-30</dcterms:created>
    <dcterms:creator>Maike Jansen</dcterms:creator>
    <dcterms:description>This is a minimal core Ontology Design Pattern defining what a DPP is, i.e. information about a product.</dcterms:description>
    <dcterms:license>https://github.com/LiUSemWeb/DPP/blob/master/LICENSE</dcterms:license>
    <dcterms:title>Digital Product Passport Ontology Design Pattern</dcterms:title>
    <vann:preferredNamespacePrefix>dpp-odp</vann:preferredNamespacePrefix>
    <vann:preferredNamespaceUri>http://w3id.org/dppo/ontology/dpp-odp/</vann:preferredNamespaceUri>
    <rdfs:seeAlso rdf:resource="https://github.com/LiUSemWeb/DPPO"/>
    <owl:versionInfo>0.1</owl:versionInfo>
  </rdf:Description>
  <rdf:Description rdf:about="http://w3id.org/dppo/ontology/dpp-odp/Product">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:nodeID="n2fca7741851243148e19e27afca19d1bb2"/>
    <rdfs:comment xml:lang="en">A product, in a broad sense, defined as anything that is sold by a value chain actor, and which can have other products as parts.</rdfs:comment>
    <rdfs:label xml:lang="en">Product</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://w3id.org/dppo/ontology/dpp-odp/hasPart">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">A relation between a whole and its parts.</rdfs:label>
    <rdfs:label xml:lang="en">has part</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://w3id.org/dppo/ontology/dpp-odp/describes">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://w3id.org/dppo/ontology/dpp-odp/DPP"/>
    <rdfs:range rdf:resource="http://w3id.org/dppo/ontology/dpp-odp/Product"/>
    <rdfs:comment xml:lang="en">The relation between a DPP and the product it describes.</rdfs:comment>
    <rdfs:label xml:lang="en">describes</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://w3id.org/dppo/ontology/dpp-odp/DPP">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:nodeID="n2fca7741851243148e19e27afca19d1bb1"/>
    <rdfs:comment xml:lang="en">A Digital Product Passport, containing information about a product, and which main contain sub-DPPs as its parts.</rdfs:comment>
    <rdfs:label xml:lang="en">Digital Product Passport</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n2fca7741851243148e19e27afca19d1bb1">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="http://w3id.org/dppo/ontology/dpp-odp/hasPart"/>
    <owl:allValuesFrom rdf:resource="http://w3id.org/dppo/ontology/dpp-odp/DPP"/>
  </rdf:Description>
